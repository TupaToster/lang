DEFCMD (PLUS, "+", 1)

DEFCMD (MINUS, "-", 1)

DEFCMD (MULT, "*", 1)

DEFCMD (DIV, "/", 1)

DEFCMD (LB, "(", 0)

DEFCMD (RB, ")", 2, {

    if (Token->type == LB) {

        Token++;
        Get_1 (Tree, iter, token);
        assert (Token->type == RB);
        Token++;
    }
    else call_next (tree, iter, token);
})

