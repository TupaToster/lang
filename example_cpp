int comp (int a, int b) {

    if (a == b) {

        return 1;
    }
    else {

        return 0;
    }
}

int Cnt = 0;

int main () {

    for (int i = 0; i < 19; i = i + 1) {

        for (int j = 20; j >= 0; j = j - 1) {

            if (comp (i, j) == 1) {

                return 1;
            }
        }
    }
    else {

        return 0;
    }
}